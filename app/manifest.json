{
  "name": "Task timer",
  "description": "This extension set timer for tasks.",
  "version": "1.2.0",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "offline_enabled": true,

  "permissions": [
    "notifications",
    "storage"
  ]
}
